{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/aacotroneo/laravel-saml2.git"
    }],
    "require": {
        "php": "^7.1.3",
        "aacotroneo/laravel-saml2": "dev-remove_mcrypt",
        "artisaninweb/laravel-soap": "0.3.0.9",
        "barryvdh/laravel-snappy": "^0.4.6",
        "brexis/laravel-workflow": "^1.2",
        "doctrine/dbal": "^2.9",
        "emadadly/laravel-uuid": "^1.2",
        "fideloper/proxy": "^4.0",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "jenssegers/model": "^1.2",
        "joselfonseca/laravel-tactician": "^0.4.1",
        "laravel/framework": "5.7.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravie/html": "~5.1",
        "league/flysystem": "1.0.8",
        "maatwebsite/excel": "^3.1",
        "mpociot/laravel-apidoc-generator": "^2.1",
        "spatie/array-to-xml": "^2.8",
        "spatie/laravel-permission": "^2.12",
        "tymon/jwt-auth": "^1.0.0",
        "yajra/laravel-oci8": "5.7.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",    
            "wsTitulus\\":  "app/Library/src/wsTitulus"          
        }
    },
    "autoload-dev": {        
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [         
      
        ]       
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
