{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.2",
    "aacotroneo/laravel-saml2": "^2.1",
    "artisaninweb/laravel-soap": "0.3.0.10",
    "barryvdh/laravel-snappy": "^1.0.0",
    "carlos-meneses/laravel-mpdf": "^2.1",
    "doctrine/dbal": "^3.0",
    "globalcitizen/php-iban": "^4.0",
    "guzzlehttp/guzzle": "^7.7",
    "h4cc/wkhtmltopdf-amd64": "0.12.x",
    "jenssegers/model": "^1.5",
    "laravel/framework": "^12.0",
    "league/flysystem": "^3.0",
    "maatwebsite/excel": "^3.1.33",
    "spatie/array-to-xml": "^2.8",
    "spatie/laravel-permission": "^6.0",
    "tymon/jwt-auth": "^2.0.0",
    "yajra/laravel-oci8": "^12.0",
    "zerodahero/laravel-workflow": "^6.1"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^8.1",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "wsTitulus\\": "app/Library/src/wsTitulus",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": []
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
